{
	"name": "fromholdio/silverstripe-configured-multisites",
	"description": "Allows for multiple websites to be managed through a single site tree.",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "multisites", "subsites"],
	"homepage": "http://github.com/fromholdio/silverstripe-configured-multisites",
	"authors": [
        {"name": "Luke Fromhold", "email": "luke@fromhold.io"},
	    {"name": "Marcus Nyeholt", "email": "marcus@symbiote.com.au"}
	],
	"require":
	{
		"silverstripe/cms": "~5.0",
		"silverstripe/admin": "~2.0",
		"symbiote/silverstripe-multivaluefield": "~6.0"
	},
    "replace": {
        "symbiote/silverstripe-multisites": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Fromholdio\\ConfiguredMultisites\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
